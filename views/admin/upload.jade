extends admin-layout
block content  
  .container#form-container
    form(id="fileUpload" action="/data/upload", method="post", enctype="multipart/form-data")    
      h2 Upload Data File
      .form-group
        label(for="username") Username
        select.form-control(name="username")
          option(value="value1") Value 1
          option(value="value2") Value 2
          option(value="value3") Value 3
      .form-group    
        label(for="file") Select a file:
        input.form-control(name="file" type="file")    
      button.btn.btn-primary(type="submit") Submit
