script.
  //- Draw SVG
  var w = 1000;
  var h = 600;  
  var svg = d3.select('#barChart')
    .append('svg')
    .attr({
      id: 'barChart',
      width: w,
      height: h
  });
  
  //- load data and call constructor functions
  var dataset;
  d3.csv('/data/jobTitles.csv', function(data) {
    dataset = data;
    console.log(dataset);
    var bars = svg.selectAll('rect')
      .data(dataset)
      .enter()
      .append('rect')
      .attr({
        x: 0,
        y: function(d, i) { return i * 21 },
        width: function(d) { return d.Percent * 1000 },
        height: 20
      });
      
  //- End d3.csv function
  });
  
